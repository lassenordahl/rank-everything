{
  "name": "@rank-everything/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:prod": "playwright test --config=playwright.prod.config.ts",
    "e2e:prod:headed": "playwright test --config=playwright.prod.config.ts --headed",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@rank-everything/qrcode": "workspace:*",
    "@rank-everything/shared-types": "workspace:*",
    "@rank-everything/validation": "workspace:^",
    "@tanstack/react-query": "^5.17.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.26",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.330.0",
    "partysocket": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "happy-dom": "^20.0.11",
    "msw": "^2.12.4",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0",
    "vitest": "^4.0.16"
  }
}
