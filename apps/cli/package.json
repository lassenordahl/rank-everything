{
  "name": "@rank-everything/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "rank": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "start": "node dist/index.js",
    "rank": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "db:migrate": "tsx src/index.ts db:migrate",
    "db:seed": "tsx src/index.ts db:seed",
    "db:reset": "tsx src/index.ts db:reset"
  },
  "dependencies": {
    "@rank-everything/shared-types": "workspace:*",
    "@rank-everything/db-schema": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "prompts": "^2.4.2",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
